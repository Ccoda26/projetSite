<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        <link rel="stylesheet" href="{{ asset('asset/css/style.css') }}">
    </head>
    <body>
        <header>
            <div class="rond">

            </div>
            <ul>
                <li> <a href="{{ path('HomePage') }}">Acceuil</a></li>
                <li><a href="{{ path('All_Articles') }}">Articles</a></li>
                <li>Items 3</li>
                <li>Items 4</li>
            </ul>
        </header>
        <main>
            {% block content %}
                {% for article in articles %}

                       <h2>{{ article.title }}</h2>


                {% endfor %}

                <div class="picture">
                <img src="{{ asset('asset/img/picture.jpg') }}" class="resp">
                </div>
            {% endblock %}
        </main>
        <footer>
            <div class="rond">

            </div>
        </footer>
    </body>
</html>
